var element =  document.getElementById("ddt_question_row_<%=params[:ddt_question_id]%>");
if (typeof(element) != 'undefined' && element != null)
{
  document.getElementById("ddt_question_row_<%=params[:ddt_question_id]%>").className= "";
  document.getElementById("ddt_question_row_<%=params[:ddt_question_id]%>").removeChild(document.getElementById("remove_link_<%=params[:ddt_question_id]%>"));
  var x = document.getElementById("ddt_question_row_<%=params[:ddt_question_id]%>").insertCell(1);
  x.innerHTML='<%= link_to (t "add_to_study"), add_ddt_question_study_path(@study, :ddt_question_id => params[:ddt_question_id]), :remote => true, :method => :post, :class => "smooth-scroll btn btn-primary " %>';
  x.id='<%= "add_link_#{params[:examiner_id]}"%>';
}
