<div class='container'>
  <div class='row'>
      <div class='col-md-3'>
      </div>
      <div class='col-md-6 col-sm-6'>
        <div class='panel panel-primary '>
          <div class="panel-heading">
              <h3 class="panel-title">
                <%= t 'registrations_list' %>
              </h3>
          </div>
          <table class='table table-striped table-bordered text-center'>
            <thead>
              <tr>
                <th class='text-center'><%= t 'clinic'%></th>
                <th class='text-center'><%= t 'examiner'%></th>
              </tr>
            </thead>

            <tbody>
              <% @registrations.each do |registration| %>
                <tr>
                  <td><%= registration.clinic.name %></td>
                  <td><%= registration.user.profile.name %></td>
                </tr>
              <% end %>
            </tbody>
          </table>
        <div class='text-center'>
        </div>
        </div>
      <div class='col-md-3'>
      </div>
    </div>
  </div>
</div>