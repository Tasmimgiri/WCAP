var element =  document.getElementById("user_row_<%=params[:examiner_id]%>");
if (typeof(element) != 'undefined' && element != null)
{
  document.getElementById("user_row_<%=params[:examiner_id]%>").className= "success";
  document.getElementById("user_row_<%=params[:examiner_id]%>").removeChild(document.getElementById("add_link_<%=params[:examiner_id]%>"));
  var x = document.getElementById("user_row_<%=params[:examiner_id]%>").insertCell(2);
  x.innerHTML='<%= link_to (t "remove_from_study"), remove_examiner_clinic_path(@clinic, :examiner_id => params[:examiner_id]), :remote => true, :method => :delete, :class => "smooth-scroll btn btn-danger " %>';
  x.id='<%= "remove_link_#{params[:examiner_id]}" %>';
}


